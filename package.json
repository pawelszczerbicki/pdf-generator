  },
  "dependencies": {
    "@decorators/di": "^1.0.1",
    "archiver": "^2.1.0",
    "aws-sdk": "^2.176.0",
    "ejs": "^2.5.7",
    "html-pdf": "^2.2.0",
    "s3-upload-stream": "^1.0.7",
